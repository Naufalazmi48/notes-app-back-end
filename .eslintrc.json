{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style" : ["off", "windows"],
        "no-underscore-dangle" : "off",
        "no-console" : "off",
        "eol-last" : "off"
    }
}
